{
  "name": "@stackflow/plugin-history-sync",
  "version": "1.5.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/daangn/stackflow.git",
    "directory": "extensions/plugin-history-sync"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "yarn build:js && yarn build:dts",
    "build:dts": "tsc --emitDeclarationOnly",
    "build:js": "node ./esbuild.config.js",
    "clean": "rimraf dist",
    "dev": "yarn build:js --watch && yarn build:dts --watch && yarn test --watch",
    "test": "yarn jest",
    "typecheck": "tsc --noEmit"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "index.ts",
      "index.tsx"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    }
  },
  "dependencies": {
    "history": "^5.3.0",
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "@stackflow/core": "^1.0.12",
    "@stackflow/esbuild-config": "^1.0.2",
    "@stackflow/react": "^1.1.10",
    "@swc/core": "^1.6.6",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "esbuild": "^0.23.0",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "rimraf": "^3.0.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "@stackflow/core": "^1",
    "@stackflow/react": "^1",
    "@types/react": ">=16.8.0",
    "react": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "ultra": {
    "concurrent": [
      "dev",
      "build"
    ]
  }
}
