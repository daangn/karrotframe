{
  "name": "@karrotframe/plugin",
  "version": "0.3.0",
  "description": "extension for karrotframe",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/karrotframe.git",
    "directory": "packages/plugin"
  },
  "license": "Apache-2.0",
  "author": "Usher <usher@daangn.com>",
  "type": "module",
  "source": "./src/index.ts",
  "main": "./src/index.ts",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "main": "./lib/index.mjs"
  },
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "build": "nanobundle build",
    "clean": "rimraf lib",
    "prepack": "yarn clean && yarn build"
  },
  "devDependencies": {
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "nanobundle": "^0.0.24",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  }
}
